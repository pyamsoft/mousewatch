    "lint": "eslint ."
  },
  "engines": {
    "node": ">=12.x"
  },
  "dependencies": {
    "@runmeetly/cache-man": "0.2.1",
    "axios": "0.24.0",
    "discord.js": "12.5.1",
    "dotenv": "8.2.0",
    "eslint": "7.16.0",
    "luxon": "1.25.0",
    "node-fetch": "2.6.1",
    "pg": "8.7.1"
  },
  "devDependencies": {
    "prettier": "2.2.1"
  }
}
