{
  "name": "mousewatch",
  "version": "1.0.3",
  "main": "index.js",
  "license": "Apache-2.0",
  "scripts": {
    "start": "yarn lint && node --trace-warnings ./src/index.js",
    "deploy:main": "yarn lint && bash ./bin/deploy 1",
